{
  "name": "webpack-stats",
  "version": "0.2.0",
  "description": "The module of reporting to the console of webpack stats data.",
  "keywords": [
    "mornya",
    "vessel",
    "node",
    "webpack",
    "stats",
    "console",
    "reporter",
    "formatter",
    "consoleize",
    "front-end",
    "back-end"
  ],
  "homepage": "",
  "license": "ISC",
  "author": "mornya <mornya78@gmail.com>",
  "private": false,
  "repository": "https://bitbucket.org/mornya-web/webpack-stats",
  "bugs": "",
  "main": "./dist/index.js",
  "types": "./dist",
  "scripts": {
    "setup": "husky install && lintest install || exit 0",
    "prepublishOnly": "vessel prepublish",
    "login": "vessel login",
    "clean": "vessel clean",
    "build": "vessel build",
    "watch": "vessel build --watch",
    "check": "vessel check",
    "lint": "lintest lint",
    "lint:fix": "lintest lint --fix",
    "test": "lintest test",
    "test:watch": "lintest test --watch",
    "test:coverage": "lintest test --coverage",
    "sonar": "sonar-scanner"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "files": [
    "dist",
    "!dist/test",
    "!dist/**/@setup.d.ts",
    "!dist/**/@setup.js",
    "!dist/**/*.test.d.ts",
    "!dist/**/*.test.js",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "type": "commonjs",
  "engines": {
    "node": "12.x || 14.x || 16.x",
    "npm": ">=6"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "cliui": "^7.0.4"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.16.3",
    "eslint": "^8.7.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.2.0",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "npm run check",
      "npm run lint"
    ]
  },
  "vessel": {
    "minVersion": "5.0.0"
  }
}
